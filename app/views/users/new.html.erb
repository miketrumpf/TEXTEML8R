<header>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                  <h2> Sign Up!</h2>
                   <div class=logIn>

<%= form_for @user do |f| %>
  <%= f.label :username %><br>
  <%= f.text_field :username%><br>
  <br>
  <%= f.label :password %><br>
  <%= f.password_field :password %><br>
  <br>
  <%= f.label :phone_number %><br>
  <%= f.text_field :phone_number %><br>
  <br>

  <%= f.submit %>

<% end %>
</div>

</header>