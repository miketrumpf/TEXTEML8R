<header>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="intro-text">
                        <span class="name">Add a Contact to Send Suite messages to!</span>
                    </div>
                </div>
            </div>
        </div>

    <%= form_for @contact do |f| %>
      <%= f.label :name %><br>
      <%= f.text_field :name%><br>

      <%= f.label :phone_number %><br>
      <%= f.text_field :phone_number%><br>
      <br>

      <%= f.submit %>


    <% end %>
</header>



